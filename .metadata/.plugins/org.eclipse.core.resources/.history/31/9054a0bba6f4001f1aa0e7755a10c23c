package ch01.ex01;

public class PrintExample {

	public static void main(String[] args) {
		
		System.out.print("Hello, Java");
		System.out.print("Hello, Java");
		System.out.println();
		System.out.print("Hello, Java");
		System.out.print("Hello, Java");
		System.out.println();
		
		System.out.printf("Hello, Java\n");
		System.out.printf("Hello, Java");
		
		int a = 10;
		int b = 20;
		double c  = 10.257;
		
		System.out.println();
		System.out.println("a="+a+", b=" + b +" c=" + c);
		
		
		System.out.printf("a=%d, b=%d,c=%f\n", a, b,c );
		System.out.printf("a=%d, b=%d,c=%.2f\n", a, b,c );
		System.out.println();

		int a1 = 10;
		int a2 = 100;
		int a3 = 1000;
		int a4 = 10000;
		int a5 = 100000;
		
		int b1 = 10;
		int b2 = 100;
		int b3 = 1000;
		int b4 = 10000;
		int b5 = 100000;
		
		System.out.println("a1="+a+", b1="+b1);
		

	}

}
