package ch01.ex04;

public class OperatorExample {

	public static void main(String[] args) {
		
		
		int a = 10;
		int b = 3;
		
		/* 정수 연산 정수 >> 정수 ex>5/2=2 나머지1은내림
		 * 정수 연산 실수 >> 실수 ex>5/2.0 =2.5
		 * 실수 연산 실수 >> 실수 ex > 5.0/ 2.0 =2.5
		 * 정수 연산 문자열 >> 문자열 
		 * 실수 연산 문자열 >> 문자열
		 */
		
		System.out.println(a+b);
		System.out.println(a-b);
		System.out.println(a*b);
		System.out.println(a/b);
		System.out.println(a%b);
		
		
		

	}

}
