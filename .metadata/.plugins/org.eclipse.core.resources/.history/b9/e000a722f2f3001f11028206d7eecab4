package ch02.sec01;

public class ValueableUserExam {

	public static void main(String[] args) {
		
		/* 
		 * 변수명작성
		 * 영문자, 숫자 _  -> 단 숫자가 맨앞에올수는 없다
		 */
		
     	int hour = 3;
		int minute = 5;
		System.out.println(hour+"시간 "+ minute +"분");
		
		int totalMinute = (hour*60) + minute;
		System.out.println("총 "+ totalMinute + " 분");

	}

}
