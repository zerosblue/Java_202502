package ch02.sec01;

public class booleanExample {

	public static void main(String[] args) {
		
		boolean stop = true;
		
		stop = false;
	
		//boolean check = 10 >= 5;
		
	
		/*    ==	같음	
				!=	다름	
				>	초과	
				<	미만	
				>=	이상	
				<=	이하	
				&& : 그리고
				|| : or
				*/
		boolean check = (5>2) && (5>11);
		System.out.println(check);
		
		
	
		

	}

}
